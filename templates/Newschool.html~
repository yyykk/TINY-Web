    <!-- JS -->
    <script>
	  function insRow(){
		var tab = document.getElementById('myTable');
		var x= tab.insertRow(tab.rows.length);
		for (i = 0; i < tab.rows.item(0).cells.length; i++){	
			var y = x.insertCell(i);
			y.innerHTML = "new";
		}
	}
    </script>
